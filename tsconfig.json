{
	"compilerOptions": {
		"target": "ES2017",
		"module": "NodeNext",
		"rootDir": "./src",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": [
		"node_modules",
		"dist",
		"privat",
		"uploads",
		"prisma",
	]
}